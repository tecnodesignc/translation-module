{
  "name": "tecnodesignc/translation-module",
  "description": "Module containing all EncoreCms translations",
  "type": "encore-module",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "composer/installers": "~1.0",
    "league/csv": "~7.2.0",
    "venturecraft/revisionable": "^1.35"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "3.8.*"
  },
  "autoload": {
    "psr-4": {
      "Modules\\Translation\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Translation\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "authors": [
    {
        "name": "Marcos Rativa",
        "email": "marcos21.009@gmail.com",
        "role": "Developer"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
