{
    "name": "tecnodesignc/translation-module",
    "type": "encore-module",
    "description": "Module containing all EncoreCms translations",
    "keywords": [
        "EncoreCMS",
        "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "info@tecnodesign.com.co",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/encorecms/issues",
        "source": "https://github.com/tecnodesignc/translation-module"
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/installers": "^2.1",
        "league/csv": "~7.2.0",
        "venturecraft/revisionable": "^1.39"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Translation\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Translation\\": ".",
            "Modules\\": "modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
